{
  "name": "@pwabuilder/cloudapk",
  "version": "1.1.0",
  "description": "Web service that packages a Progressive Web App as an Android Trusted Web Activity app for publishing in the Google Play Store.",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node ./server.js",
    "postinstall": "tsc",
    "dev": "cross-env NODE_ENV=development node --inspect=5858 --loader ts-node/esm ./server.ts",
    "build": "tsc --noEmitOnError",
    "docker:build": "npm run build && docker build -t cloud-apk .",
    "docker:run": "docker run -p 5779:5858 cloud-apk"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pwa-builder/pwabuilder-google-play.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pwa-builder/pwabuilder/issues"
  },
  "homepage": "https://github.com/pwa-builder/pwabuilder-google-play/",
  "dependencies": {
    "@azure/identity": "^4.12.0",
    "@azure/storage-blob": "^12.28.0",
    "@bubblewrap/core": "^1.24.0",
    "@resvg/resvg-js": "^2.4.1",
    "@types/archiver": "^3.0.0",
    "@types/cors": "^2.8.13",
    "@types/escape-html": "^1.0.4",
    "@types/express": "^4.17.2",
    "@types/express-fileupload": "^1.1.3",
    "@types/fs-extra": "^8.0.1",
    "@types/q": "^1.5.2",
    "@types/tmp": "^0.1.0",
    "applicationinsights": "^2.9.0",
    "archiver": "^5.3.1",
    "body-parser": "^1.20.2",
    "concurrently": "^5.1.0",
    "cors": "^2.8.5",
    "del": "^7.0.0",
    "dotenv": "^16.0.3",
    "escape-html": "^1.0.3",
    "express": "^4.21.2",
    "fs-extra": "^11.1.0",
    "ioredis": "^5.8.0",
    "lodash": "^4.17.21",
    "multer": "2.0.2",
    "node-fetch": "^3.3.1",
    "password-generator": "^2.3.2",
    "path": "^0.12.7",
    "q": "^1.5.1",
    "rimraf": "^5.0.1",
    "tmp": "^0.2.4",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4",
    "unzip2": "^0.2.5"
  },
  "devDependencies": {
    "cross-env": "^10.1.0"
  }
}
