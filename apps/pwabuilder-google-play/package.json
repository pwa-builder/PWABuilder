    "node-forge": ">=0.7.2",
    "qs": ">=6.3.2",
    "path-parse": ">=1.0.7",
    "fstream": ">=1.0.12",
    "dot-prop": ">=5.1.1",
    "hoek": ">=5.0.3",
    "vm2": ">=3.9.8",
    "async": ">=3.2.2",
    "hawk": ">=9.0.1",
    "minimatch": ">=3.0.2"
  }
}
