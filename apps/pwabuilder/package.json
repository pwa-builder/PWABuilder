{
  "name": "pwabuilder",
  "version": "0.0.1",
  "description": "A starter kit for building PWAs!",
  "main": "index.js",
  "scripts": {
    "preinstall": "node ../../scripts/setupDeps.js",
    "test": "npm install -D playwright && npx playwright install-deps chromium && npx playwright test",
    "mocha": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha",
    "coverage": "nyc npm run mocha",
    "clean": "rimraf build/ && rimraf dist/",
    "build": "rimraf dist/ && cross-env NODE_ENV=production && rollup --config rollup.config.mjs",
    "start": "npm run build && npx nodemon server.js",
    "serve": "npx nodemon server.js",
    "dev": "cross-env NODE_ENV=development && concurrently \"rollup --config rollup.config.dev.mjs --watch\" \"web-dev-server\"",
    "dev:deps": "cross-env NODE_ENV=development && concurrently \"node ../../scripts/watchDeps.js\" \"rollup --config rollup.config.dev.mjs --watch\" \"web-dev-server\"",
    "debug": "concurrently \"rollup --config rollup.config.dev.mjs --watch\" \"web-dev-server --config web-dev-server.debugging.config.mjs\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@pwabuilder/manifest-editor": "file:../../components/manifest-editor",
    "@pwabuilder/code-editor": "file:../../components/code-editor",
    "@pwabuilder/pwaauth": "file:../../components/pwa-auth",
    "@pwabuilder/manifest-previewer": "^1.0.21",
    "@pwabuilder/manifest-validation": "file:../../libraries/manifest-validation",
    "@pwabuilder/pwainstall": "^1.6.6",
    "@pwabuilder/site-analytics": "file:../../libraries/site-analytics",
    "@vaadin/router": "^1.7.4",
    "browser-fs-access": "^0.23.0",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "lit": "^2.0.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@open-wc/rollup-plugin-html": "^1.2.5",
    "@playwright/test": "^1.12.3",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-strip": "^2.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/chai": "^4.2.21",
    "@types/lodash-es": "^4.17.4",
    "@types/mocha": "^8.2.3",
    "@types/service_worker_api": "^0.0.9",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "@web/dev-server": "^0.1.17",
    "chai": "^4.3.4",
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.3",
    "danger": "^10.6.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "lit-analyzer": "^1.2.1",
    "mocha": "^9.2.2",
    "msw": "^0.33.0",
    "nyc": "^15.1.0",
    "playwright": "^1.17.2",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.44.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-lit-css": "^2.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-version-injector": "^1.3.3",
    "ts-node": "^10.1.0",
    "tslib": "^2.1.0",
    "typescript": "^4.3.2"
  }
}
